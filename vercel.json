{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/api/tuner.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/backend/api/tuner.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
